    "    i+=1\n",
    "    while(i<N and X[i]<=s+R):\n",
    "        i+=1\n",
    "    p=X[i-1]\n",
    "    print(p)\n",
    "    while(i<N and X[i]<=p+R):\n",
    "        i+=1\n",
    "    ans += 1\n",
    "print('result: {0}'.format(ans))\n"
