    "def solve(A,coins):\n",
    "    vals=(500,100,50,10,5,1)\n",
    "    ans = 0\n",
    "    for val,coin in zip(vals, coins):\n",
    "        t = min(A//val,coin)\n",
    "        A -= val*t\n",
    "        ans += t\n",
    "        #print(t)\n",
    "    return ans"
   ]
